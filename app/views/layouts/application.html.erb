<html>
  <head>
    <title>Shelter - Community when you need it most</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slideout/1.0.1/slideout.min.js"></script>
    <%= stylesheet_link_tag 'main.css', media: 'all' %>
    <%= stylesheet_link_tag 'index.css', media: 'all' %>
    <%= javascript_include_tag 'jquery.min.js' %>
    <%= javascript_include_tag 'main.js' %>
    <%= javascript_include_tag 'skel.min.js' %>
    <%= javascript_include_tag 'util.js' %>
    <%= javascript_include_tag 'menu.js' %>
    <%= content_tag("div", nil, :id => 'js-properties', :data => {:properties => all_coordinates}) %>
    <%= javascript_include_tag 'map.js', type: "text/javascript"%>
    <script src="//maps.google.com/maps/api/js?key=<%= ENV["API_KEY"] %>"></script>
    <script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=<%= ENV["API_KEY"] %>&callback=initMap"
    type="text/javascript">
    </script>
  </head>
  <body>

    <%= render "layouts/flash" %>

    <div id="wrapper">
      <header id="header">
        <div class="inner">

          <%= render "layouts/navbar1" %>
          <!-- <%= render "layouts/navmenu" %> -->
          <!-- this ^^ is for the JS menu slideout-menu -->

        </div>
      </header>

    	<div id="main">
				<div class="inner">

          <%= yield %>

				</div>
    	</div>

      <%= render "layouts/footer" %>

      </div>
    </body>
</html>
